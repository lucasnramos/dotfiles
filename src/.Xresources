! =====================
! [START] Rofi
! =====================
rofi.modi:             ssh,window,drun,run
rofi.sidebar-mode:     true
rofi.terminal:         st
rofi.ssh-client:       ssh
rofi.ssh-command:      {terminal} -e "{ssh-client} {host}"
rofi.opacity:          100
rofi.width:            50
rofi.lines:            8
rofi.columns:          1
rofi.font:             Inconsolata 14
rofi.theme:            solarized

! =====================
! [START] MISC
! =====================
Xft.dpi:             96
Xft.antialias:       true
Xft.rgba:            rgb
Xft.hinting:         true
Xft.hintstyle:       hintslight
