super + Return
    $TERMINAL
super + F1
    $BROWSER
super + F2
    code
super + F5
    pavucontrol

# dmenu | rofi shortcuts
super + d
    rofi -show drun
super + c
    exec rofi-dotfiles
super + r
    rofi -show run
super + w
    rofi -show window

# Media Controls
XF86AudioRaiseVolume
    amixer set Master 5%+
XF86AudioLowerVolume
    amixer set Master 5%-
