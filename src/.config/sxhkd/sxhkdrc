    urxvtc
super + F1
    $BROWSER
super + F2
    code
super + F5
    pavucontrol
super + ctrl + p
    killall -q compton
super + shift + q
    dmenu-exit
    
# dmenu
super + {d,p}
    dmenu_run
super + c
    exec dmenu-dotfiles
super + control + c
    exec dmenu-suckless-config
super + ctrl + p
    exec dmenu-projects
super + r
    rofi -show run
super + w
    rofi -show window

