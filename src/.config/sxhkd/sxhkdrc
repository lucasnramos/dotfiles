super + Return
    $TERMINAL
super + F1
    $BROWSER
super + F2
    code
super + F3
    $TERMINAL -e nvim
super + F4
    pavucontrol
super + F5
    lutris
super + F6
    steam
super + F7
    retroarch
super + shift + e
    $TERMINAL -e $FILEMANAGER
super + e
    $GFM
super + ctrl + p
    killall -q compton
super + control + q
    dmenu-exit
    
# dmenu
super + c
    exec dmenu-dotfiles
super + control + c
    exec dmenu-suckless-config
super + ctrl + p
    exec dmenu-projects

# Rofi
super + Tab
  rofi -show window
super + p
  rofi -show run
