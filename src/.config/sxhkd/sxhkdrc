super + Return
    $TERMINAL
super + F1
    $BROWSER
super + F2
    code
super + F5
    pavucontrol
super + ctrl + p
    killall -q compton
XF86AudioRaiseVolume
    amixer set Master 5%+
XF86AudioLowerVolume
    amixer set Master 5%-
    

# dmenu | rofi shortcuts
super + d
    rofi -show drun
super + c
    exec rofi-dotfiles
super + r
    rofi -show run
super + shift + w
    rofi -show window
