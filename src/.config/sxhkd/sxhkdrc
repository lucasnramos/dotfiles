super + Escape
  pkill -USR1 -x sxhkd
super + Return
  $TERMINAL
super + {_, shift} + F1
  { google-chrome-stable,firefox }
super + {_, shift} + e
  { thunar, $TERMINAL -e ranger }
super + ctrl + p
  killall -q compton
super + control + q
  dmenu-exit
  
# dmenu
super + c
  exec dmenu-dotfiles
super + control + c
  exec dmenu-suckless-config
super + ctrl + p
  exec dmenu-projects

# Rofi
super + Tab
  exec rofi -show window
super + shift + p
  rofi -modi lpass:rofi-lpass -show lpass
