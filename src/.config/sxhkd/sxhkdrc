super + return
    $TERMINAL
super + Return
    $TERMINAL
super + F1
    $BROWSER
super + F2
    code
super + F5
    pavucontrol
super + ctrl + p
    killall -q compton
XF86AudioRaiseVolume
    amixer set Master 5%+
XF86AudioLowerVolume
    amixer set Master 5%-
super + shift + e
    spacefm
    

# dmenu
super + {d,p}
    dmenu_run
super + c
    exec dmenu-dotfiles
super + ctrl + p
    exec dmenu-projects
super + r
    rofi -show run
super + w
    rofi -show window
